<div class="grid grid-cols-3 gap-3 mt-5">
	<div class="bg-white p-3 rounded h-20">
		<span class="flex space-x-10">
		    <svg class="w-10 h-10 ml-2 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path></svg>
		    <p class="text-2xl font-semibold text-gray-500"><%= @memberships.count %></p>
		</span>
		<h5 class="text-center mt-1 text-gray-500">Total Members</h5>
	</div>
	<div class="bg-white p-3 rounded h-20">
		<span class="flex space-x-10">
			<svg class="w-10 h-10 ml-2 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path></svg>
			<p class="text-2xl font-semibold text-gray-500"><%= @membermale %></p>
		</span>
		<h5 class="text-center mt-1">Total Males</h5>
	</div>
	<div class="bg-white p-3 rounded h-20">
		<span class="flex space-x-10">
			<svg class="w-10 h-10 ml-2 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path></svg>
			<p class="text-2xl font-semibold text-gray-500"><%= @memberfemale %></p>
		</span>
		<h5 class="text-center mt-1">Total Females</h5>
	</div>
</div>

<div class="mt-12">
	<div class="inline-block min-w-full shadow rounded-lg overflow-hidden mt-5 border-t-2 border-gray-500">
	<table class="w-full bg-white">
	<thead>
		<tr class="border-b-2 border-gray-200">
			<th class="text-left px-3 py-2">Id</th>
			<th class="text-left px-3 py-2">Branch Name</th>
			<th class="text-left px-3 py-2">Leader's Name</th>
		</tr>
	</thead>
	<tbody>
	  <% @branches.each do |branch| %>
		<tr class="border-b border-gray-200 hover:bg-gray-100">
			<td class="px-3 py-1"><%= branch.id %></td>
			<td class="px-3 py-1"><%= branch.branchname %></td>
			<td class="px-3 py-1"><%= branch.leaders_name %></td>
		</tr>
	</tbody>
	<% end %>
  </table>
 </div>
</div>


